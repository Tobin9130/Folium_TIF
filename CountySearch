import pandas as pd
import folium, os, json

from folium import plugins

path = os.getcwd()

with open(os.path.join(path, 'Counties_v17a.geojson')) as f:
    counties = json.loads(f.read())


m = folium.Map(
    location=[44.3148, -84.6024],
    tiles='cartodbpositron',
    zoom_start=7,
)

plugins.Search(counties,search_label='NAME', search_zoom=3, geom_type='polygon').add_to(m)


m.save('map.html')
